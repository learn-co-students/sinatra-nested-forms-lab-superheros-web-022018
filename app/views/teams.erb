<h1>Team Name: <%= @team[:name] %></h1>
<h2>Team Motto: <%= @team[:motto] %></h2>

<% @team[:heroes].each_with_index do |hero, index| %>
    <h3>Hero <%= index + 1 %></h3>
    <ul>
      <li>Hero Name: <%= hero[:name] %></li>
      <li>Hero Power: <%= hero[:power] %></li>
      <li>Hero Biography: <%= hero[:bio] %></li>
    </ul>
<% end %>
